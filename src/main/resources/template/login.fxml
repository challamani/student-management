<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="600.0" maxWidth="1000.0" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="service.LoginService">
   <children>
      <Button fx:id="loginLabel" layoutX="418.0" layoutY="356.0" mnemonicParsing="false" onAction="#Login" prefHeight="39.0" prefWidth="110.0" text="Login">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font></Button>
      <TextField fx:id="username" layoutX="283.0" layoutY="235.0" prefHeight="31.0" prefWidth="245.0" promptText="username">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font></TextField>
      <PasswordField fx:id="password" layoutX="283.0" layoutY="298.0" prefHeight="11.0" prefWidth="245.0" promptText="Password">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font></PasswordField>
      <Separator layoutX="31.0" layoutY="180.0" prefHeight="21.0" prefWidth="938.0" />
      <Label fx:id="userLabel" layoutX="193.0" layoutY="240.0" text="Username">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font>
      </Label>

      <Label fx:id="passLabel" layoutX="195.0" layoutY="303.0" text="Password">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font>
      </Label>
      <Label fx:id="signIn" alignment="CENTER" contentDisplay="CENTER" layoutX="374.0" layoutY="133.0" prefHeight="47.0" prefWidth="154.0" text="Sign In" textFill="#23662c">
         <font>
            <Font name="Chalkboard" size="36.0" />
         </font>
      </Label>

      <Label fx:id="status" layoutX="195.0" layoutY="475.0" prefHeight="39.0" prefWidth="287.0" textFill="#d41b00de" />

      <Label fx:id="welcomeId" alignment="CENTER" layoutX="458.0" prefHeight="76.0" prefWidth="535.0" text="Student Management System" textAlignment="CENTER">
         <font>
            <Font name="Chalkboard" size="36.0" />
         </font>
      </Label>
   </children>
</AnchorPane>

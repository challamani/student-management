<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane maxHeight="600.0" maxWidth="1000.0" prefHeight="600.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="service.MainController">
   <children>
      <Label layoutX="469.0" layoutY="33.0" text="Student Management System" textFill="#000000b6">
         <font>
            <Font name="Chalkboard" size="36.0" />
         </font>
      </Label>

      <Button fx:id="add_student" layoutX="48.0" layoutY="120.0" mnemonicParsing="false" onAction="#addStudent" prefHeight="50.0" prefWidth="213.0" text="Add">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font>
      </Button>
      <Button fx:id="view_student" layoutX="48.0" layoutY="170.0" mnemonicParsing="false" onAction="#viewStudent" prefHeight="50.0" prefWidth="213.0" text="View">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font>
      </Button>
      <Button fx:id="update_student" layoutX="48.0" layoutY="220.0" mnemonicParsing="false" onAction="#updateStudent" prefHeight="50.0" prefWidth="213.0" text="Update">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font>
      </Button>

      <Separator layoutX="48.0" layoutY="99.0" prefHeight="0.0" prefWidth="904.0" />

      <Button fx:id="add_fee" layoutX="48.0" layoutY="270.0" mnemonicParsing="false" onAction="#addFee" prefHeight="50.0" prefWidth="213.0" text="Add Fee">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font>
      </Button>
      <Button fx:id="update_fee" layoutX="48.0" layoutY="320.0" mnemonicParsing="false" onAction="#updateFee" prefHeight="50.0" prefWidth="213.0" text="Update Fee">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font>
      </Button>
      <Button layoutX="48.0" layoutY="370.0" mnemonicParsing="false" onAction="#feeReport" prefHeight="50.0" prefWidth="213.0" text="Report">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font>
      </Button>
      <Label fx:id="loginLabel" layoutX="702.0" layoutY="114.0" prefHeight="28.0" prefWidth="244.0" textFill="#28a100" />

      <Button fx:id="exitButton" layoutX="48.0" layoutY="420.0" mnemonicParsing="false" prefHeight="50.0" prefWidth="213.0" text="Exit">
         <font>
            <Font name="Chalkboard" size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
